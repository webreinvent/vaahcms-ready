{
    "name": "laravel/laravel",
    "type": "project",
    "description": "VaahCMS is a web application development platform shipped with headless content management system.",
    "keywords": [
        "framework",
        "vaahcms",
        "cms",
        "headless cms",
        "hmvc",
        "vue",
        "vuex",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "creativeorange/gravatar": "^1.0",
        "doctrine/dbal": "^2.13",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.3",
        "intervention/image": "^2.5",
        "laravel/framework": "8.41.0",
        "laravel/tinker": "^2.6",
        "zanysoft/laravel-zip": "^1.0",
        "webreinvent/laravel-cpanel": "^1.3",
        "webreinvent/vaahcms": "^1.3",
        "webreinvent/vaahextend": "dev-develop",
        "ext-json": "*",
        "ext-imap": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "facade/ignition": "^2.9",
        "fakerphp/faker": "^1.14",
        "laravel/sail": "^1.5",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^5.4",
        "phpunit/phpunit": "^9.5",
        "laravel/dusk": "^6.15"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "VaahCms\\Modules\\": "VaahCms/Modules/",
            "VaahCms\\Themes\\": "VaahCms/Themes/"
        },
        "classmap": [
            "database/seeders",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
